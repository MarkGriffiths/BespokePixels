{
  "name": "@thebespokepixel/sublime-theme-bespokepixels",
  "version": "0.0.1",
  "description": "Sublime Text 3 theme using latests specs",
  "main": "gulpfile.js",
  "repository": "https://MarkGriffiths@github.com/MarkGriffiths/BespokePixels.git",
  "scripts": {
    "test": "xo && gulp && nyc ava",
    "readme": "compile-readme -u source/docs/example.md source/docs/readme.md > readme.md"
  },
  "keywords": [
    "sublime",
    "theme"
  ],
  "author": "Mark Griffiths <mark@thebespokepixel.com> (http://thebespokepixel.com/)",
  "license": "MIT",
  "devDependencies": {
    "@thebespokepixel/badges": "^1.0.2",
    "@thebespokepixel/oco-colorvalue-ex": "^1.0.0",
    "@thebespokepixel/palette2oco": "^1.0.1",
    "@thebespokepixel/string": "^0.5.4",
    "ava": "^1.2.1",
    "fancy-log": "^1.3.3",
    "gulp": "4.0.0",
    "gulp-data": "^1.2.1",
    "gulp-include": "^2.3.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^5.0.0",
    "gulp-yaml": "^2.0.3",
    "js-yaml": "^3.6.1",
    "lodash": "^4.13.1",
    "nyc": "^13.3.0",
    "opencolor": "^0.2.0",
    "plist": "^3.0.1",
    "plugin-error": "^1.0.1",
    "through2": "^3.0.0",
    "trash": "^5.0.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "semicolon": false,
    "ignores": [
      "test/fixtures/**",
      "dist/**"
    ]
  },
  "badges": {
    "github": "MarkGriffiths",
    "npm": "thebespokepixel",
    "name": "BespokePixels",
    "providers": {
      "status": {
        "text": "Sublime Text",
        "color": "FF9801"
      }
    },
    "readme": {
      "Status": [
        [
          "status",
          "greenkeeper",
          "travis",
          "david-dev",
          "gitter"
        ]
      ]
    }
  }
}