{
  "name": "@thebespokepixel/sublime-theme-bespokepixels",
  "version": "0.0.1",
  "description": "Sublime Text 3 theme using latests specs",
  "main": "gulpfile.js",
  "repository": "https://github.com/thebespokepixel/BespokePixels.git",
  "scripts": {
    "test": "xo && gulp && nyc ava",
    "readme": "compile-readme -u source/docs/example.md source/docs/readme.md > readme.md"
  },
  "keywords": [
    "sublime",
    "theme"
  ],
  "author": "Mark Griffiths <mark@thebespokepixel.com> (http://thebespokepixel.com/)",
  "license": "MIT",
  "devDependencies": {
    "@thebespokepixel/badges": "^3.0.0",
    "@thebespokepixel/oco-colorvalue-ex": "^4.0.3",
    "@thebespokepixel/palette2oco": "^1.0.6",
    "@thebespokepixel/string": "^0.5.10",
    "ava": "^2.3.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-data": "^1.2.1",
    "gulp-include": "^2.4.1",
    "gulp-load-plugins": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^5.0.0",
    "gulp-yaml": "^2.0.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "nyc": "^14.1.1",
    "opencolor": "^0.2.0",
    "plist": "^3.0.1",
    "plugin-error": "^1.0.1",
    "through2": "^3.0.1",
    "trash": "^6.0.0",
    "xo": "^0.24.0"
  },
  "ava": {
    "helpers": [
      "**/helpers/**/*"
    ]
  },
  "xo": {
    "semicolon": false,
    "ignores": [
      "test/helpers/**",
      "dist/**"
    ]
  },
  "badges": {
    "github": "thebespokepixel",
    "npm": "thebespokepixel",
    "name": "BespokePixels",
    "providers": {
      "status": {
        "text": "Sublime Text",
        "color": "FF9801"
      }
    },
    "readme": {
      "Status": [
        "status",
        "greenkeeper",
        "travis-com",
        "david-devdeps"
      ],
      "Help": [
        "twitter"
      ]
    }
  }
}
